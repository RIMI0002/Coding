//Write a program in C to find the sum of two numbers.
#include<stdio.h>

    int sum(float n)
    {
        float s=0;
        s=(n*(n+1))/2;
        printf("The sum of n numbers:%f\n",s);
    }
    int main()
    {
        float n=0,s=0;
        printf("Enter the value of n:\n");
        scanf("%f",&n);
        sum(n);
    }
